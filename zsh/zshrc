export ZSH=/home/nkn/.oh-my-zsh
export GOPATH=$HOME/go

PATH=`sed -e '/^#/'d -e '/^$/'d << EOF | paste -d ":" -s 
$PATH
$GOPATH/bin
/usr/local/go/bin
EOF`


ZSH_THEME="bureau"

plugins=(git rvm node rvm ubuntu composer docker go httpie laravel4 laravel5 npm bower virtualenv vagrant docker-compose pip pep8 sudo frontend-search git-extras httpie)

source $ZSH/oh-my-zsh.sh

[ -s "$HOME/.rvm/scripts/rvm" ] && . "$HOME/.rvm/scripts/rvm" 
[ -s "$HOME/.dnx/dnvm/dnvm.sh" ] && . "$HOME/.dnx/dnvm/dnvm.sh" # Load dnvm

