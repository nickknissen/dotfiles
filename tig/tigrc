bind status a !?git commit --amend
bind status P !git push origin
bind status D !@?rm %(file)

bind main F !sh -c "git commit --fixup=%(commit) && git rebase -i --autosquash --autostash %(commit)~"
bind main R !git rebase -i %(commit)^ --autostash

bind generic C !git commit -v

bind stash A !?git stash apply %(stash)  # apply selected stash
